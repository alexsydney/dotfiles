[user]
	name = M.Shibuya
	email = mit.shibuya@gmail.com
[color]
	ui = true
[core]
	excludesfile = /Users/shibuya/.gitignore
	editor = /usr/local/bin/vim
[alias]
	diffc = diff --cached
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = simple
